{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "importHelpers": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "target": "ES2017",
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
  },
}
